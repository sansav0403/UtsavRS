#import <Foundation/Foundation.h>
#import "RSFolio.h"
#import "RSLocation.h"
@interface RSSpaLocations : NSObject
{

    NSMutableArray *spaLocations;
    Result *spaLocationResult;
}

@property(nonatomic,retain) NSMutableArray *spaLocations;
@property(nonatomic,retain) Result *spaLocationResult;

@end



@interface RSSpaLocation : NSObject {
    
    NSString *locationName;
    int locationID;
    
    RSLocation *location;
    NSString *imageUrl;
    
    BOOL allowGender;
    BOOL allowStaff;
    BOOL allowOverBook;
    
    NSString *bookingType;
    
    NSString *mondayStart;
    NSString *mondayEnd;
    
    NSString *tuesdayStart;
    NSString *tuesdayEnd;
    
    NSString *wednesdayStart;
    NSString *wednesdayEnd;
    
    NSString *thrusdayStart;
    NSString *thrusdayEnd;
    
    NSString *fridayStart;
    NSString *fridayEnd;
    
    NSString *saturdayStart;
    NSString *saturdayEnd;
    
    NSString *sundayStart;
    NSString *sundayEnd;
}

@property(nonatomic,copy) NSString *locationName;

@property(nonatomic) int locationID;

@property(nonatomic, retain) RSLocation *location;
@property(nonatomic, copy) NSString *imageUrl;

@property(nonatomic) BOOL allowGender;
@property(nonatomic) BOOL allowStaff;
@property(nonatomic) BOOL allowOverBook;

@property(nonatomic, copy) NSString *bookingType;

@property(nonatomic, copy) NSString *mondayStart;
@property(nonatomic, copy) NSString *mondayEnd;

@property(nonatomic, copy) NSString *tuesdayStart;
@property(nonatomic, copy) NSString *tuesdayEnd;

@property(nonatomic, copy) NSString *wednesdayStart;
@property(nonatomic, copy) NSString *wednesdayEnd;

@property(nonatomic, copy) NSString *thrusdayStart;
@property(nonatomic, copy) NSString *thrusdayEnd;

@property(nonatomic, copy) NSString *fridayStart;
@property(nonatomic, copy) NSString *fridayEnd;

@property(nonatomic, copy) NSString *saturdayStart;
@property(nonatomic, copy) NSString *saturdayEnd;

@property(nonatomic, copy) NSString *sundayStart;
@property(nonatomic, copy) NSString *sundayEnd;
@end
